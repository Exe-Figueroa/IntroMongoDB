// Aggregation
db.listingsAndReviews.aggregate([
  {
    $match: {
      amenities: "Wifi"
    }
  },
  {
    $project: {
      address: 1,
      amenities: 1,
      price: 1,
    }
  },
  {
    $group: {
      _id: "$address.country",
      count: { $sum: 1 }
    }
  }
])

